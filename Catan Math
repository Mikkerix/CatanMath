}
console.log("hello, developer!");
const form = document.getElementById("form");
form.addEventListener("submit", function (e) {
  e.preventDefault();
  var Sum_av_tretti_seks = 0;
  console.log("", Sum_av_tretti_seks);
  var melding = "You should get resources ";
  var melding2 = " times of 36 different possibilities, or ";
  var melding3 =
    "% of the time. We hope you enjoyed Catan Math, if you wanna play Catan online we recommend: ";
  const tall2 = document.getElementById("tall2").value;
  const tall1 = document.getElementById("tall1").value;
  const tall3 = document.getElementById("tall3").value;
  console.log(tall1);
  var muligheter = [];
  for (var i = 1; i < 7; i++) {
    for (var n = 1; n < 7; n++) {
      muligheter.push(i, n);
      if (i + n == tall1 || i + n == tall2 || i + n == tall3) {
        Sum_av_tretti_seks += 1;
      }
    }
  }
  var Prosentandel = (parseInt(Sum_av_tretti_seks) / 36) * 100;
  var ProsentandelAvrundet = Prosentandel.toFixed(2);
  const output = document.getElementById("output");
  document.write(melding);
  document.write(Sum_av_tretti_seks);
  document.write(melding2);
  document.write(ProsentandelAvrundet);
  document.write(melding3);
  document.write("https://www.colonist.io");
});
